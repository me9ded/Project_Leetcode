class Solution:
    def swapPairs(self, head: Optional[ListNode]) -> Optional[ListNode]:
        dummy=ListNode(0,head)
        prev,head=dummy,head
        while curr and curr.next:
            next_pair=curr.next.next
            second=curr.next

            second.next=curr
            curr.next=next_pair
            prev.next=second

            prev=curr
            curr=next_pair
        return dummy.next